{
  "extends": "google",
  "installedESLint": true,
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "browser": true
  },
  "rules": {
    "no-unused-vars": "off",
    "max-len": "off",
    "space-before-function-paren": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off"
  }
}
